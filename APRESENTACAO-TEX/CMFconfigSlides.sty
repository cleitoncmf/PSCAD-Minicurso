\newcommand{\vodq}{
\left[
\begin{array}{c}
\tilde{v}_{o}^{d}\\
\tilde{v}_{o}^{q}\\
\tilde{v}_{o}^{0}
\end{array}
\right]
}


\newcommand{\iodq}{
\left[
\begin{array}{c}
\tilde{i}_{o}^{d}\\
\tilde{i}_{o}^{q}\\
\tilde{i}_{o}^{0}
\end{array}
\right]
}

\newcommand{\wcross}{
\left[
\begin{array}{ccc}
0 & -\omega_0 & 0\\
\omega_0  & 0 & 0\\
0 & 0 & 0
\end{array}
\right]
}




