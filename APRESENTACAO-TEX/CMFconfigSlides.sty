\newcommand{\vdcsigmadq}{
\left[
\begin{array}{c}
\tilde{v}_{dc}^{d\Sigma}\\
\tilde{v}_{dc}^{q\Sigma}\\
\tilde{v}_{dc}^{0\Sigma}
\end{array}
\right]
}

\newcommand{\vdcdeltadq}{
\left[
\begin{array}{c}
\tilde{v}_{dc}^{d\Delta}\\
\tilde{v}_{dc}^{q\Delta}\\
\tilde{v}_{dc}^{0\Delta}
\end{array}
\right]
}


\newcommand{\vdcdq}{
\left[
\begin{array}{c}
0\\
0\\
\tilde{v}_{dc}^{0}
\end{array}
\right]
}

\newcommand{\icirdq}{
\left[
\begin{array}{c}
\tilde{i}_{cir}^{d}\\
\tilde{i}_{cir}^{q}\\
\tilde{i}_{cir}^{0}
\end{array}
\right]
}

\newcommand{\icdq}{
\left[
\begin{array}{c}
\tilde{i}_{c}^{d}\\
\tilde{i}_{c}^{q}\\
\tilde{i}_{c}^{0}
\end{array}
\right]
}


\newcommand{\ecirdq}{
\left[
\begin{array}{c}
\tilde{e}_{cir}^{d*}\\
\tilde{e}_{cir}^{q*}\\
\tilde{e}_{cir}^{0*}
\end{array}
\right]
}


\newcommand{\ecdq}{
\left[
\begin{array}{c}
\tilde{e}_{c}^{d*}\\
\tilde{e}_{c}^{q*}\\
\tilde{e}_{c}^{0*}
